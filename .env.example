# MGNREGA Dashboard Environment Variables
# Copy this file to .env and update the values

# Database Configuration
DATABASE_URL=postgresql://mgnrega_user:mgnrega_pass@postgres:5432/mgnrega_db
POSTGRES_PASSWORD=mgnrega_pass

# Redis Configuration
REDIS_URL=redis://redis:6379

# API Keys (REQUIRED)
# Get your API key from: https://data.gov.in/
DATA_GOV_API_KEY=your-data-gov-api-key-here

# JWT Secret (REQUIRED for production)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# Worker Configuration
POLL_INTERVAL_HOURS=24

# Production Configuration (for docker-compose.prod.yml)
DOMAIN=your-domain.com

# Optional: Frontend URL for CORS
FRONTEND_URL=http://localhost:3000
